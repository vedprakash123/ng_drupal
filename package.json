{
  "name": "n-gapp-universal",
  "version": "0.0.0",
  "scripts": {
    "build:dynamic": "npm run build:ssr && npm run serve:ssr",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --output-hashing none && ng run my-test:server --configuration=prod --progress=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
